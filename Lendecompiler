import java.util.Scanner;
import java.util.*;

 class Dcoder
 {
   public static void main(String args[])
   { 
    
    Scanner scan1 = new Scanner(System.in);
    
    String word;
    
    System.out.print("Use len compiler to make a word that will be decompiled \n \n");
    
    System.out.print("Enter word to decompile : ");
                
    word = scan1.nextLine();
    
    char[] d = word.toCharArray();
    
    int length = d.length;
                      
     List<Character> wor = new ArrayList<Character>();                 
      
                           
     for(int i = 0; i < length; i++){
       System.out.println("");
       if(d[i]=='-'){
         wor.add('h');
       }else if(d[i]=='$'){
         wor.add('o');
       }else if(d[i]=='#'){
         wor.add('l');
       }else if(d[i]=='?'){
         wor.add('c');
       }else if(d[i]=='☆'){
         wor.add('e');
       }else if(d[i]=='¿'){
         wor.add('x');
       }else if(d[i]=='&'){
         wor.add('n');
       }else if(d[i]=='^'){
         wor.add('a'); 
       }else if(d[i]=='÷'){
         wor.add('i');
       }else if(d[i]=='◇'){
         wor.add('g');
       }else if(d[i]=='□'){
         wor.add('r');
       }else if(d[i]=='%'){
         wor.add('u');                    
       }else if(d[i]=='µ'){
         wor.add('/');
       }else if(d[i]=='¡'){
         wor.add(' ');
       }
       else if(d[i]=='¤'){
         //void
       }else if(d[i]=='_'){
         //void
       }else if(d[i]=='~'){
         //void
       }else if(d[i]=='*'){
         //void
       }else if(d[i]=='@'){
         //void
       }else{
         wor.add(d[i]);
       }
       
       
       
         for(int p = 0; p < wor.size(); p++){
          System.out.print(wor.get(p));
          try        
          {
          Thread.sleep(10);
          } 
          catch(InterruptedException ex) 
          {
          Thread.currentThread().interrupt();
          }
       }
       
     }
                                                         
    }
 }

